{
  "manifest_version": 2,
  "name": "Strava Total Speed",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://www.strava.com/activities/*"
      ],
      "js": ["jquery-2.2.4.min.js", "content.js"]
    }
  ],
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}